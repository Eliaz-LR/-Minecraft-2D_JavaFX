<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.Generation"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Label layoutX="10.0" layoutY="10.0" text="Generation du monde :" />
        <Button fx:id="genPetit" layoutX="10.0" layoutY="40.0" minWidth="100.0" text="Petit" onAction="#genPetit" />
        <Button fx:id="genMoyen" layoutX="10.0" layoutY="70.0" text="Moyen" onAction="#genMoyen" />
        <Button fx:id="genGrand" onAction="#genGrand" layoutX="10.0" layoutY="100.0" prefHeight="25.0" prefWidth="100.0" text="Grand" />
    </children>
</AnchorPane>
