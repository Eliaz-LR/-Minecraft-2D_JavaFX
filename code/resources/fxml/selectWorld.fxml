<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.Generation"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="10.0" layoutY="10.0" text="Generation du monde :" />
        <Label layoutX="10.0" layoutY="40.0" text="Taille horizontale :" />
        <Spinner fx:id="width" />
        <Label layoutX="10.0" layoutY="70.0" text="Taille verticale :" />
        <Spinner fx:id="height" />
        <Button fx:id="valider_gen" onAction="#handleGenerate" layoutX="10.0" layoutY="100.0" prefHeight="25.0" prefWidth="100.0" text="Valider" />
    </children>
</AnchorPane>
